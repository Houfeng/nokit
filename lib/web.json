{
    "port": 8000,
    "defaults": ["index.html", "index.nsp"],
    "mimeType": {
        "*": "application/octet-stream",
        ".html": "text/html",
        ".htm": "text/html",
        ".nsp": "text/html",
        ".md": "text/markdown",
        ".css": "text/css",
        ".less": "text/less",
        ".txt": "text/plain",
        ".js": "text/javascript",
        ".json": "application/x-javascript",
        ".png": "image/png",
        ".jpg": "image/jpg",
        ".jpeg": "image/jpeg",
        ".gif": "image/gif",
        ".bmp": "image/bmp",
        ".ico": "image/x-icon"
    },
    "headers": {},
    "handlers": {
        "*": "./handlers/static",
        "^/__res__/": "./handlers/resource",
        "^/--res--/": "./handlers/resource",
        ".nsp": "./handlers/nsp",
        ".md": "./handlers/markdown",
        ".less": "./handlers/less",
        ".css": "./handlers/css"
    },
    "parsers": {
        "*": "./parsers/urlencoded",
        "multipart/form-data": "./parsers/multipart",
        "application/x-www-form-urlencoded": "./parsers/urlencoded",
        "text/plain": "./parsers/plain",
        "text/json": "./parsers/json"
    },
    "global": "./global.js",
    "filters": {},
    "responsePages": {
        "500": "./templates/500.html",
        "404": "./templates/404.html",
        "405": "./templates/405.html",
        "dir": "./templates/dir.html"
    },
    "folders": {
        "public": "./public"
    },
    "cache": {
        "client": {
            "match": [".html$", ".htm$", ".css$", ".js$", ".icon$", ".ico$", ".less$", ".sass$", ".md$", ".png$", ".jpg$", ".jpeg$", ".gif$", ".bmp$", ".swf$"],
            "maxAge": 1800
        },
        "server": {
            "match": [".html$", ".htm$", ".css$", ".js$", ".icon$", ".ico$", ".less$", ".sass$", ".md$"]
        }
    },
    "denys": [".nsp.js$", "web.json$", "global.js$", "package.json$", ".tp$", ".tp.html$", ".tp.htm$", ".log$"],
    "session": {
        "state": true,
        "timeout": 1800,
        "provider": "./core/session"
    },
    "displayDir": true,
    "log": {
        "provider": "./core/logger",
        "path": "./logs"
    }
}