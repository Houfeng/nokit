{
    "hosts": [],
    "port": 8000,
    "defaults": ["index.html", "index.nsp"],
    "headers": {},
    "handlers": {
        "*": "./handlers/static",
        "^/__res__/": "./handlers/resource",
        "^/--res--/": "./handlers/resource",
        "^/favicon.icon$": "./handlers/favicon",
        "^/favicon.ico$": "./handlers/favicon",
        ".nsp": "./handlers/nsp",
        ".md": "./handlers/markdown",
        ".less": "./handlers/less",
        ".css": "./handlers/css"
    },
    "parsers": {
        "*": "./parsers/urlencoded",
        "multipart/form-data": "./parsers/multipart",
        "application/x-www-form-urlencoded": "./parsers/urlencoded",
        "text/plain": "./parsers/plain",
        "text/json": "./parsers/json",
        "text/xml": "./parsers/xml"
    },
    "global": "./global.js",
    "filters": {},
    "template": {
        "pages": {
            "500": "./templates/500.html",
            "404": "./templates/404.html",
            "403": "./templates/403.html",
            "405": "./templates/405.html",
            "dir": "./templates/dir.html"
        },
        "compress": true
    },
    "folders": {
        "public": "./public"
    },
    "cache": {
        "client": {
            "match": [".html$", ".htm$", ".css$", ".js$", ".json$", ".xml$", ".svg$", ".txt$", ".text$", ".csv$", ".icon$", ".ico$", ".less$", ".sass$", ".md$", ".png$", ".jpg$", ".jpeg$", ".gif$", ".bmp$", ".swf$", ".ttf$", ".woff$", ".eot"],
            "maxAge": 1800,
            "modifiedSince": true
        }
    },
    "denys": [".nsp.js$", "web.json$", "global.js$", "package.json$", ".tp$", ".tp.html$", ".tp.htm$", ".log$"],
    "session": {
        "state": true,
        "timeout": 1800,
        "provider": "./core/session"
    },
    "compress": [".html$", ".htm$", ".css$", ".js$", ".json$", ".xml$", ".svg$", ".txt$", ".text$", ".md$", ".csv$"],
    "displayDir": true,
    "log": {
        "provider": "./core/logger",
        "path": "./logs"
    },
